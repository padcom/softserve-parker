    "db-migrate-mysql": "^1.1.10",
    "express": "^4.17.1",
    "graphql": "^14.4.2",
    "helmet": "^3.18.0",
    "jsonwebtoken": "^8.5.1",
    "lodash.get": "^4.4.2",
    "mysql2": "^1.7.0",
